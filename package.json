{
  "name": "task-bird",
  "version": "1.0.0",
 "description": "create tasks, assign and email",
  "main": "index.js",
  "engines": {
    "node": "12.8.2",
    "npm": "6.14.5"
  },
  "scripts": {
    "start:dev": "concurrently \"npm run start:server:dev\" \"npm run start:client:dev\"",
    "start:client:dev": "cd client && npm start",
    "start:server:dev": "nodemon --exec babel-node index.js"
  },
  "keywords": ["Node, Mongo, GraphQL, React"],
